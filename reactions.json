[
  {
    "id": "hydrogenation",
    "name": "Hydrogenation",
    "category": "Addition",
    "difficulty": "Easy",
    "description": "Addition of H2 across C=C or C≡C double/triple bonds. Converts unsaturated hydrocarbons to saturated ones.",
    "equation": "C=C + H2 → C-C (with H additions)",
    "conditions": {
      "temperature": "25-100°C",
      "pressure": "1-100 atm",
      "catalyst": "Ni, Pd, or Pt"
    },
    "pattern": {
      "requiresDoubleBond": true,
      "requiresCarbon": true,
      "carbonCount": "any"
    },
    "products": {
      "transformation": "Convert C=C to C-C, add H atoms"
    },
    "mechanism": [
      {
        "step": 1,
        "description": "Alkene π electrons interact with catalyst"
      },
      {
        "step": 2,
        "description": "H2 bonds break on catalyst surface"
      },
      {
        "step": 3,
        "description": "Hydrogen atoms add to both carbons of former double bond"
      }
    ]
  },
  {
    "id": "dehydration_alcohol",
