    "category": "Elimination",
    "difficulty": "Easy",
    "description": "Removes water (H2O) from alcohols to form alkenes. R2CH-OH → R2C=C + H2O",
    "equation": "R-CH2-OH → R-CH=CH2 + H2O",
    "conditions": {
      "temperature": "140-180°C",
      "catalyst": "H2SO4 or H3PO4",
      "solvent": "None (heat)"
    },
    "pattern": {
      "requiresAlcohol": true,
      "carbonCount": "2+"
    },
    "products": {
      "transformation": "Remove OH, create C=C double bond"
    },
    "mechanism": [
      {
        "step": 1,
        "description": "Acid protonates hydroxyl group"
      },
      {
        "step": 2,
        "description": "Water departs, leaving carbocation"
      },
      {
        "step": 3,
        "description": "Neighboring hydrogen is removed, forming alkene"
      }
    ]
  },
  {
    "id": "hydration_alkene",
    "name": "Hydration of Alkene",
    "category": "Addition",
    "difficulty": "Easy",
    "description": "Adds water across C=C to form alcohols. Markovnikov's rule applies.",
    "equation": "C=C + H2O → CHOH-CH2 or CH2OH-CH",
    "conditions": {
      "temperature": "25°C",
      "catalyst": "H2SO4 or H3PO4",
      "solvent": "Water or aqueous acid"
    },
    "pattern": {
      "requiresDoubleBond": true,
      "requiresCarbon": true
    },
    "products": {
      "transformation": "Convert C=C to C-C, add OH and H"
    },
    "mechanism": [
      {
        "step": 1,
        "description": "π electrons attack H+ from acid"
      },
      {
        "step": 2,
        "description": "Carbocation forms (more substituted carbon)"
      },
      {
        "step": 3,
        "description": "Water attacks carbocation"
      },
      {
        "step": 4,
        "description": "Deprotonation gives alcohol"
      }
    ]
  },
