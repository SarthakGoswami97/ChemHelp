    "category": "C-C Bond Formation",
    "difficulty": "Hard",
    "description": "Two aldehydes or ketones react to form β-hydroxy carbonyl compound.",
    "equation": "R-CHO + R'-CHO → R-CH(OH)-CH(R')-CHO",
    "conditions": {
      "temperature": "Room temperature",
      "catalyst": "NaOH or KOH",
      "solvent": "Water or ethanol"
    },
    "pattern": {
      "requiresAldehydeOrKetone": true,
      "hasAlphaHydrogen": true,
      "carbonCount": "1+"
    },
    "products": {
      "aldol": "β-hydroxy carbonyl"
    },
    "mechanism": [
      {
        "step": 1,
        "description": "Base deprotonates α-carbon, forming enolate anion"
      },
      {
        "step": 2,
        "description": "Enolate attacks carbonyl carbon of second molecule"
      },
      {
        "step": 3,
        "description": "Alkoxide intermediate forms"
      },
      {
        "step": 4,
        "description": "Protonation gives β-hydroxy carbonyl (aldol product)"
      }
    ]
  },
